
tf-content
  position relative
  display block
  min-height 100%
  padding 20px
  padding-top header-height + 20px
  box-sizing border-box
  color black

  .candidates
    border-bottom 1px solid #DDD

    padding-bottom 10px
    margin-bottom 10px

    .candidate
      font-family "Cookie"

      padding 0px 20px
      margin-right 5px
      margin-left 5px
      font-size 24pt
      display inline-block

      background #EEE
      border-radius 1000px

      sub, sup
        font-family "Muli"
        font-size 10pt

