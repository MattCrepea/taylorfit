
.questions(data-bind="css:{active:active}")
  span.title Settings
  span.statlist
    header
      span.title Goodness-of-Fit Statistics
    // ko foreach:allstats
    // ko if:!global && !$data.default
    div.stat(data-bind="text:name,css:{selected:selected},\
    click:function(){selected(!selected())}")
    // /ko
    // /ko
  tf-counter.multiplicands(params="name:'Max Multiplicands',value:multiplicands,max:max_multiplicands")
  tf-multiselect.exponents(params="name:'Exponents',values:exponents,fixed:['1']")
  tf-multiselect.lags(params="name:'Lags',values:lags")
  span.title Advanced
  tf-checkbox(params="name:'Always Show Lags',value:show_lags")
  tf-counter(params="name:'Precision',value:ko.precision,min:1,max:9")
  tf-checkbox(params="name:'Subscribe to Changes',value:subscribedToChanges")

  span.button.warning(data-bind="click:clear_project")
    | Clear Project
  span.button.warning(data-bind="click:clear_model")
    | Clear Model
  //span.button.export-dataset(data-bind="click:download_dataset")
  //  | Export Data
  span.button.export-model(data-bind="click:download_model")
    | Export Project
  a.button.manual(href="resources/TaylorFit Manual 2017.pdf" target="window")
    | Open Users' Manual

