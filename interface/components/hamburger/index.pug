
.questions(data-bind="css:{active:active}")
  span.title Settings
  span.statlist
    header
      span.title Display Statistics
    // ko foreach:allstats
    // ko if:!global && sort
    div.stat(data-bind="text:name,css:{selected:selected},\
    click:function(){selected(!selected())}")
    // /ko
    // /ko
  tf-counter.multiplicands(params="name:'Max No. of Multiplicands',value:multiplicands,max:9")
  tf-pills.exponents(params="name:'Exponents',values:exponents,style:'check',fixed:['1']")
  tf-pills.lags(params="name:'Lags',values:lags,style:'check'")

  tf-other-settings.other-settings(params="name:'Advanced',values:otherSettings")

  tf-exporter(params="model:model")
