
- var rand = function ( lo, hi ) {
-   return Math.floor(lo + Math.random() * (hi - lo))
- }

doctype html
html(lang="en-us")
  head
    meta(charset="utf-8")
    link(rel="stylesheet" href="./index.css")
  body
    section.section#grid
      header
        span Data Input
        span.small Start by inputting some data
        span.import.button.right Import CSV
      .help
      table
        - var cols = [1,2,3,4,5,6]
        - var rows = [1,2,3,4,5,6]
        thead
          tr
            each col in cols
              th= "Part " + col
        tbody
          each row in rows
            tr
              each col in cols
                td= rand(10, 100)
    section.section#rows
      header
        span Row Selection
        span.small Click and drag table rows or input data
      input(type="text" value=rand(1,3))
      span.space -
      input(type="text" value=rand(4,6))

